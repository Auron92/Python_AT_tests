# inner modules
    {% if inner_modules %}
    {% for inner_module in inner_modules %}
from .{{ inner_module | module_local_name }} import {{ inner_module | module_self_class_name }}
    {% endfor %}
    {% endif +%}

# inner files
    {% if inner_files %}
    {% for inner_file in inner_files %}
from .{{ inner_file.file_name[:-3] }} import {{ inner_file.class_name }}
    {% endfor %}
    {% endif %}


{#from .{{ self_class_name | sc_local_name }} import {{ self_class_name | sc_class_name }}#}


{% if inner_files %}
__all__ = (
        {% for inner_file in inner_files %}
    '{{ inner_file.class_name }}',
        {% endfor %}
)
{% endif %}
